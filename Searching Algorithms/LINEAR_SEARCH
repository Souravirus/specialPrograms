
#include<iostream>
using namespace std;


int main(){
    
    int N,M;
    
    cin>>N;
    int a[N];
    for(int i=0;i<N;i++){
        cin>>a[i];
    }
    ///Take input the element to search
    cin>>M;
    
    ///Linear Search
    int i;
    
    for(i=0;i<N;i++){
        if(a[i]==M){
            cout<<i;
            break;
        }
    }
    if(i==N){
        cout<<"-1";
    }
    return 0;
}
